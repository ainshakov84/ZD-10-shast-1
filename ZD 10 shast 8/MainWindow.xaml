<Window x:Class="ZD_10_shast_8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZD_10_shast_8"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="800">
    <StackPanel Margin="0,0,0,-95">
        <Ellipse Fill="LightBlue" Width="200" Height="200" StrokeThickness="5" StrokeDashArray="4 2"
                 Stroke="Gray" StrokeDashCap="Round"/>
        <Rectangle Fill="LightCoral" Width="200" Height="100" RadiusX="15" RadiusY="15" Margin="10"/>
        <Line X1="100" Y1="30" X2="200" Y2="150" Stroke="Red"/>
        <Polygon Fill="LightGreen" Points="100,150,200,50,300,150"/>
        <Polyline Stroke="Red" Points="100,150,200,50,300,150"/>
    </StackPanel>
</Window>
